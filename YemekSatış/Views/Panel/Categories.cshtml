
@{
    ViewBag.Title = "Categories";
    Layout = "~/Views/Shared/_PanelLayout.cshtml";
}

<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">KATEGORİLER</h4>
    </div>
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">

            <li class="active">Kategori</li>
        </ol>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="white-box">
            <form class="form-horizontal form-material" id="frmCategories">
                <div class="form-group">
                    <label class="col-md-12">Kategori Adı</label>
                    <div class="col-md-12">
                        <input type="text" name="user_name" placeholder="Kategori Adı" class="form-control form-control-line">
                    </div>
                </div>               
                <div class="form-group">
                    <div class="col-sm-12">
                        <input type="submit" class="btn btn-success" value="KAYDET" />
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

